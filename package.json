{
    "name": "pragma-views",
    "version": "0.0.59",
    "description": "Package that provides all required parts for pragma visuals",
    "main": "commonjs/src/index.js",
    "scripts": {
        "test": "nyc mocha --compilers js:babel-core/register --recursive",
        "build": "bucli -c all",
        "run": "bucli -c all && http-server",
        "browser": "start chrome http://localhost:8080",
        "build-publish": "bucli -p"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "aurelia-dialog": "^1.0.0-rc.1.0.3",
        "aurelia-event-aggregator": "^1.0.1",
        "aurelia-framework": "^1.1.2",
        "aurelia-pal-nodejs": "^1.0.0-beta.1.0.0",
        "aurelia-polyfills": "^1.2.1",
        "babel-core": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-latest": "^6.24.1",
        "chai": "^3.5.0",
        "istanbul": "^0.4.5",
        "jspm": "^0.16.53",
        "jspm-github": "^0.13.18",
        "mocha": "^3.4.2",
        "nyc": "^10.3.2",
        "sass": "^0.5.0",
        "sinon": "^2.3.1"
    },
    "babel": {
        "presets": [
            "latest"
        ],
        "plugins": [
            "transform-decorators-legacy",
            "transform-class-properties"
        ]
    },
    "nyc": {
        "check-coverage": true,
        "require": [
            "babel-register"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "include": [
            "src/**/*.js"
        ],
        "exclude": [
            "app/**/*.*",
            "node_modules/**/*.*",
            "jspm_packages/**/*.*"
        ],
        "cache": true,
        "all": true
    },
    "jspm": {
        "registry": "npm",
        "main": "index",
        "format": "amd",
        "directories": {
            "dist": "amd/src/"
        },
        "dependencies": {
            "aurelia-bootstrapper": "^2.1.1",
            "aurelia-dialog": "^1.0.0-rc.1.0.3",
            "aurelia-event-aggregator": "^1.0.1",
            "aurelia-framework": "^1.1.0",
            "aurelia-polyfills": "^1.2.2",
            "aurelia-ui-virtualization": "^1.0.0-beta.3.1.0",
            "pragma-menu": "^1.0.17"
        },
        "devDependencies": {
            "aurelia-pal-browser": "^1.2.1",
            "systemjs/plugin-text": "github:systemjs/plugin-text@^0.0.9"
        }
    },
    "dependencies": {
        "require-dir": "^0.3.2"
    }
}